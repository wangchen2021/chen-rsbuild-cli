{
  "name": "chen-rsbuild-cli-app",
  "version": "1.0.0",
  "author": "<%= author %>",
  "description": "<%= projectDesc %>",
  "private": true,
  "type": "module",
  "scripts": {
    "predev": "tsx scripts/check-node-version.ts",
    "prebuild": "tsx scripts/check-node-version.ts",
    "prepreview": "tsx scripts/check-node-version.ts",
    "preinstall:custom": "tsx scripts/check-node-version.ts",
    "build": "rsbuild build",
    "dev": "rsbuild dev --open",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "lint": "eslint .",
    "preview": "rsbuild preview",
    "prepare": "node -e \"try { require('husky'); } catch (e) { if (e.code !== 'MODULE_NOT_FOUND') throw e; }\"",
    "git:commit": "git add . && npx cz",
    "git:push": "git push",
    "release": "release-it",
    "release:patch": "release-it patch",
    "release:minor": "release-it minor",
    "release:major": "release-it major",
    "generate:api": "tsx scripts/generate-api.ts",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "engines": {
    "node": "24.x",
    "pnpm": ">=9.0.0"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.11.2",
    "axios": "^1.13.4",
    "dayjs": "^1.11.19",
    "form-data": "^4.0.5",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.13.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.4.0",
    "@commitlint/config-angular": "^20.4.0",
    "@commitlint/format": "^20.4.0",
    "@commitlint/types": "^20.4.0",
    "@epegzz/sass-vars-loader": "^6.1.0",
    "@eslint/js": "^9.39.1",
    "@react-dev-inspector/middleware": "^2.0.1",
    "@release-it/bumper": "^7.0.5",
    "@release-it/conventional-changelog": "^10.0.4",
    "@rsbuild/core": "^1.7.1",
    "@rsbuild/plugin-babel": "^1.1.0",
    "@rsbuild/plugin-node-polyfill": "^1.4.3",
    "@rsbuild/plugin-react": "^1.4.2",
    "@rsbuild/plugin-sass": "^1.5.0",
    "@rsbuild/plugin-svgr": "^1.3.0",
    "@rsdoctor/rspack-plugin": "^1.5.1",
    "@storybook/addon-a11y": "^10.2.4",
    "@storybook/addon-docs": "^10.2.4",
    "@storybook/addon-links": "^10.2.4",
    "@storybook/addon-onboarding": "^10.2.4",
    "@storybook/react": "^10.2.4",
    "@storybook/react-docgen-typescript-plugin": "^1.0.1",
    "@types/git-rev-sync": "^2.0.2",
    "@types/loader-utils": "^3.0.0",
    "@types/node": "^25.1.0",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@types/semver": "^7.7.1",
    "babel-plugin-react-compiler": "^1.0.0",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "eslint-plugin-storybook": "^10.2.4",
    "git-rev-sync": "^3.0.2",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "loader-utils": "^3.3.1",
    "openapi-typescript-codegen": "^0.30.0",
    "prettier": "^3.7.3",
    "puppeteer": "^22.15.0",
    "react-dev-inspector": "^2.0.1",
    "release-it": "^19.2.4",
    "semver": "^7.7.3",
    "storybook": "^10.2.4",
    "storybook-react-rsbuild": "^3.2.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.0",
    "typescript-plugin-css-modules": "^5.2.0",
    "webpack-merge": "^6.0.1"
  }
}
