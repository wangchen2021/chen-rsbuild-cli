{
  "name": "chen-rsbuild-cli",
  "version": "1.0.2",
  "description": "React+RSBuild+TS Chen CLI",
  "main": "dist/index.js",
  "type": "commonjs",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/index.ts",
    "prepublishOnly": "pnpm run build",
    "link:global": "pnpm run build && pnpm link --global",
    "unlink:global": "pnpm unlink --global"
  },
  "bin": {
    "chen-rsbuild-cli": "./dist/index.js"
  },
  "keywords": [
    "cli",
    "react",
    "rsbuild",
    "typescript",
    "scaffold"
  ],
  "files": [
    "dist",
    "templates",
    "README.md"
  ],
  "author": "Wang Chen",
  "license": "ISC",
  "dependencies": {
    "chalk": "^5.6.2",
    "commander": "^14.0.3",
    "download-git-repo": "^3.0.2",
    "ejs": "^4.0.1",
    "inquirer": "^13.3.0",
    "ora": "^9.3.0"
  },
  "devDependencies": {
    "@types/ejs": "^3.1.5",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^25.3.0",
    "fs-extra": "^11.3.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=24.0.0"
  }
}